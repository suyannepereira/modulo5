package com.AgeTravel.mod5.model;



@Data
@Entity
@Table(name = "compras") //ou compra no singular
public class Compra {

	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private Long id;
	
	@Column(nullable = false)
	private String dataCompra;
	
	@Column(nullable = false)
	private double total_compra;
	
	
	
	
	
}
